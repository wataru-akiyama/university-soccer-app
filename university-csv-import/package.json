{
  "name": "university-soccer-csv-firebase-import",
  "version": "1.0.0",
  "description": "CSVデータをFirebaseにインポートするツール",
  "main": "csv-to-firebase.js",
  "scripts": {
    "validate": "node validate-data.js",
    "test": "node test-conversion.js",
    "import": "node csv-to-firebase.js",
    "import-csv": "node csv-to-firebase.js './oshinagaki_data.csv'",
    "test-conversion": "node test-conversion.js",
    "update-all": "node update-all-data.js",
    "update-university": "node partial-update.js university",
    "update-comments": "node partial-update.js comments",
    "add-new": "node add-new-data.js add",
    "check-duplicates": "node add-new-data.js check",
    "test-v2": "node test-conversion-v2.js",
    "import-v2": "node csv-to-firebase-v2.js",
    "update-all-v2": "node update-all-data-v2.js",
    "update-all-v2-auto": "node update-all-data-v2.js ./oshinagaki_data2.csv --yes",
    "validate-v2": "node validate-data-v2.js oshinagaki_data2.csv",
    "quick-update-v2": "npm run validate-v2 && npm run test-v2 && npm run update-all-v2-auto"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "dotenv": "^16.3.1"
  },
  "keywords": [
    "firebase",
    "csv",
    "import",
    "university",
    "soccer"
  ],
  "author": "PLAYMAKER",
  "license": "MIT"
}